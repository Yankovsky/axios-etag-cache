{
  "name": "axios-etag-cache",
  "version": "1.0.5",
  "description": "Axios etag interceptor to enable If-None-Match request with ETAG support",
  "keywords": [
    "axios",
    "cache",
    "etag"
  ],
  "homepage": "https://github.com/raulanatol/axios-etag-cache#readme",
  "bugs": {
    "url": "https://github.com/raulanatol/axios-etag-cache/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raulanatol/axios-etag-cache.git"
  },
  "license": "ISC",
  "author": {
    "name": "raulanatol",
    "email": "raul@natol.es"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm run clean && webpack --mode production",
    "clean": "shx rm -rf dist .dts",
    "lint": "eslint --max-warnings 0 . --ext .ts,.tsx",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(css|jpg|png|svg)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/config/jest/preprocessor.js"
    }
  },
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "axios": "^0.19.2",
    "dts-bundle": "^0.7.3",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^25.1.0",
    "nock": "^12.0.1",
    "node-cache": "^5.1.0",
    "shx": "^0.3.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "peerDependencies": {
    "axios": "^0.19.2"
  },
  "pre-push": [
    "lint",
    "test",
    "build"
  ]
}
